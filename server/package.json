{
  "name": "alwaysactive-server",
  "version": "1.0.0",
  "description": "An app to connect you with events near you",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "concurrently \"tsc -w\" \"NODE_ENV=development nodemon ./build/server.js\"",
    "start": "NODE_ENV=production node build/server.js",
    "clean": "rm -r build"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.8.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
